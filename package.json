{
  "name": "kinda-vdom-exploration",
  "version": "0.1.0",
  "description": "An exploration in using a Lisp-y kind of VDOM",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "cov": "jest --coverage",
    "jsdoctest": "mocha --require jsdoctest `find src -type f -not -name \"*.spec.js\"`",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "slay-mutants": "stryker run",
    "start": "http-server -p 8081 .",
    "test": "jest && npm run jsdoctest",
    "test-ci": "NODE_ENV=test npm run test && npm run test-ui-ci",
    "test-ui": "cypress open",
    "test-ui-ci": "cypress run",
    "test-watch": "jest --watch",
    "watch": "NODE_ENV=development rollup --config rollup.config.js --watch"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --findRelatedTests"
    ]
  },
  "pre-commit": [
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfeineis/kinda-vdom-exploration.git"
  },
  "keywords": [
    "vdom",
    "javascript",
    "js"
  ],
  "author": "Martin Feineis",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mfeineis/kinda-vdom-exploration/issues"
  },
  "homepage": "https://github.com/mfeineis/kinda-vdom-exploration#readme",
  "devDependencies": {
    "cypress": "^3.1.4",
    "eslint": "^5.12.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-jest": "^22.1.3",
    "http-server": "^0.11.1",
    "jest": "^24.0.0",
    "jsdoctest": "^1.7.1",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "ramda": "^0.26.1",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.3",
    "stryker": "^0.34.0",
    "stryker-api": "^0.23.0",
    "stryker-baseline-reporter": "^1.0.3",
    "stryker-html-reporter": "^0.17.0",
    "stryker-javascript-mutator": "^0.13.0",
    "stryker-jest-runner": "^1.3.0"
  },
  "dependencies": {}
}
